    "item": [
        {
            "name": "Admin",
            "item": [
                {
                    "name": "List all careers",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{adminToken}}"
                            }
                        ],
                        "url": {
                            "raw": "{{baseUrl}}/admin/careers",
                            "host": [
                                "{{baseUrl}}"
                            ],
                            "path": [
                                "admin",
                                "careers"
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "name": "Add new career",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{adminToken}}"
                            },
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n    \"careerName\": \"Quantum Physicist\",\n    \"bucket\": \"Science & Research\",\n    \"riasecProfile\": \"I\",\n    \"primarySubjects\": \"[\\\"Physics\\\",\\\"Mathematics\\\"]\",\n    \"tags\": \"[\\\"quantum\\\",\\\"research\\\"]\",\n    \"minQualification\": \"PhD\",\n    \"top5CollegeCourses\": \"PhD in Physics,MSc in Physics\",\n    \"baseParagraph\": \"Studies the fundamental nature of reality at the subatomic level.\",\n    \"microprojects\": \"Simulate a quantum entanglement experiment.\",\n    \"whyFit\": \"Enjoys theoretical and abstract problems.\",\n    \"recommendedSkills\": [\"Quantum Mechanics\", \"Python\", \"Calculus\"],
    \"recommendedCourses\": [\"MIT OpenCourseWare Quantum Physics\"]\n}"
                        }
                    },
                    "response": []
                },
                {
                    "name": "Update career",
                    "request": {
                        "method": "PUT",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{adminToken}}"
                            },
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n    \"careerName\": \"Quantum Computing Scientist\",\n    \"bucket\": \"Science & Research\",\n    \"riasecProfile\": \"I\",\n    \"primarySubjects\": \"[\\\"Physics\\\",\\\"Mathematics\\\",\\\"Computer Science\\\"]\",\n    \"tags\": \"[\\\"quantum\\\",\\\"computing\\\",\\\"research\\\"]\",\n    \"minQualification\": \"PhD\",\n    \"top5CollegeCourses\": \"PhD in Physics,MSc in Physics, PhD in Computer Science\",\n    \"baseParagraph\": \"Researches and develops quantum computing algorithms and hardware.\",\n    \"microprojects\": \"Simulate a quantum entanglement experiment using Qiskit.\",\n    \"whyFit\": \"Enjoys theoretical and abstract problems at the intersection of physics and computer science.\",\n    \"recommendedSkills\": [\"Quantum Mechanics\", \"Python\", \"Qiskit\", \"Calculus\"],
    \"recommendedCourses\": [\"MIT OpenCourseWare Quantum Physics\", \"IBM Quantum Experience\"]\n}"
                        }
                    },
                    "response": []
                },
                {
                    "name": "Delete career",
